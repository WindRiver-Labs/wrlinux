From 55cd6badb9a698e595d974a982f3121c2c09001e Mon Sep 17 00:00:00 2001
From: Wenzong Fan <wenzong.fan@windriver.com>
Date: Tue, 3 Mar 2015 17:25:09 +0800
Subject: [PATCH] keepalived: don't strip binaries

Fix QA Errors:

QA Issue: File '/usr/bin/genhash' from keepalived was already stripped, this will prevent future debugging! [already-stripped]
QA Issue: File '/usr/sbin/keepalived' from keepalived was already stripped, this will prevent future debugging! [already-stripped]

Upstream-Status: Pending

Signed-off-by: Wenzong Fan <wenzong.fan@windriver.com>
---
 genhash/Makefile.in    | 2 --
 keepalived/Makefile.in | 2 --
 2 files changed, 4 deletions(-)

diff --git a/genhash/Makefile.in b/genhash/Makefile.in
index 2404474..5fedb84 100644
--- a/genhash/Makefile.in
+++ b/genhash/Makefile.in
@@ -12,7 +12,6 @@ mandir      = @mandir@
 datarootdir = @datarootdir@
 
 CC = @CC@
-STRIP = @STRIP@
 INCLUDES = -I../lib
 CFLAGS = @CFLAGS@ @CPPFLAGS@ $(INCLUDES) \
 	 -Wall -Wunused -Wstrict-prototypes
@@ -23,7 +22,6 @@ LIB_OBJS = ../lib/timer.o ../lib/scheduler.o ../lib/memory.o ../lib/list.o \
 	   ../lib/utils.o ../lib/html.o ../lib/signals.o ../lib/logger.o
 
 all:	$(BIN)/$(EXEC)
-	$(STRIP) $(BIN)/$(EXEC)
 	@echo ""
 	@echo "Make complete"
 
diff --git a/keepalived/Makefile.in b/keepalived/Makefile.in
index cabcc5f..ce348c4 100644
--- a/keepalived/Makefile.in
+++ b/keepalived/Makefile.in
@@ -21,7 +21,6 @@ conf_dir    = $(sysconfdir)/keepalived
 sysconf_dir = $(sysconfdir)/sysconfig
 
 CC = @CC@
-STRIP = @STRIP@
 LDFLAGS = @LIBS@ @LDFLAGS@ -ldl
 SUBDIRS = core
 
@@ -47,7 +46,6 @@ all:
 	$(MAKE) -C $$i || exit 1; done && \
 	echo "Building $(BIN)/$(EXEC)" && \
 	$(CC) -o $(BIN)/$(EXEC) `find $(SUBDIRS) ../lib -name '*.[oa]'` $(LDFLAGS)
-	$(STRIP) $(BIN)/$(EXEC)
 	@echo ""
 	@echo "Make complete"
 
-- 
1.9.1

